@model IEnumerable<CourseRegistration.Models.Participant>

<div id="employeeList">

    @foreach (var item in Model)
    {
        <div class="checkbox">
            <label>
                @item.FullName <input type="checkbox" id="@item.ParticipantId" name="@item.FullName">
            </label>
        </div>
    }

</div>